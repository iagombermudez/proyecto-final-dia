<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="gestionHotel.IU.gestionReservas.VisualizacionFactura"
        Title="Factura"
        Height="300" Width="400"
        MinHeight="300" MinWidth="400"
        MaxHeight="1000" MaxWidth="5000">
    <StackPanel Margin="5">
            
        <DockPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Name="tbFactura"></TextBlock> 
        </DockPanel> 
        <Button Name="btGuardarFactura" Margin="10" HorizontalAlignment="Center">Guardar</Button>
        
    </StackPanel>
        
</Window>