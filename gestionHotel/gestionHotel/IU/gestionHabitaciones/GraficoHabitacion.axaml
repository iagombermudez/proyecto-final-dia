<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ScottPlot="clr-namespace:ScottPlot.Avalonia;assembly=ScottPlot.Avalonia"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="gestionHotel.IU.gestionHabitaciones.GraficoHabitacion"
        MinWidth="900"
        MinHeight="600"
        Width="900"
        Height="600"
        Title="Grafico Reservas por Habitación">
    
   
    <DockPanel HorizontalAlignment="Stretch"
               VerticalAlignment="Stretch">
        <TabControl Name="TabReservas" >
            <TabItem Header="Reservas/mes" Name="TabMes"  Margin="5" FontSize="20">
                <Label><ScottPlot:AvaPlot Name="GraficaMes" MinWidth="200" MinHeight="300" /></Label>
            </TabItem>
            <TabItem Header="Reservas/años" Name="TabAnho" Margin="5" FontSize="20">
                <Label><ScottPlot:AvaPlot Name="GraficaAnho" MinWidth="200" MinHeight="300" /></Label>
            </TabItem>  
        </TabControl>
    </DockPanel>  
    
</Window>